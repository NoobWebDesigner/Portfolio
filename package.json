{
    "scripts": {
        "dev": "yarn pug:dev & yarn webpack:dev",
        "watch": "yarn pug:watch & yarn webpack:watch",
        "prod": "yarn pug:prod & yarn webpack:prod",
        "webpack:dev": "NODE_ENV=development webpack --config=node_modules/laravel-mix/setup/webpack.config.js",
        "webpack:watch": "NODE_ENV=development webpack --config=node_modules/laravel-mix/setup/webpack.config.js --watch",
        "webpack:prod": "NODE_ENV=production  webpack --config=node_modules/laravel-mix/setup/webpack.config.js",
        "pug:dev": "pug src/pug/ --hierarchy -o public -P",
        "pug:watch": "pug src/pug/ --hierarchy -o public -P -w",
        "pug:prod": "pug src/pug/ --hierarchy -o public"
    },
    "name": "laravel-mix-test",
